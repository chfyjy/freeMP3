QWidget#Widget
{
border:none;
}

QLabel#titleL
{
    border:none;
    min-width: 32px;
    min-height: 32px;
    background-position:center;
    background-repeat:no-repeat;
    background-image: url(:/pic/shitvip.png);
}

QPushButton#closeBtn
{
    border:none;
    min-width: 32px;
    min-height: 32px;
    background-position:center;
    background-repeat:no-repeat;
    background-image: url(:/pic/close.png);
}

QPushButton#closeBtn:hover
{
        background-image: url(:/pic/close1.png);
}

QPushButton#closeBtn:pressed
{
	background-image: url(:/pic/close1.png);
}


QPushButton#minBtn {
    border:none;
